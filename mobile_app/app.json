{
  "expo": {
    "name": "Predictrix",
    "slug": "predictrix",
    "scheme": "predictrix",
    "version": "0.4.3",
    "sdkVersion": "51.0.0",
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "updates": {
      "enabled": false
    },
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asuralab.predictrix"
    },
    "android": {
      "package": "com.asuralab.predictrix",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "useNextNotificationsApi": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1a237e",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "86604e92-7568-4cff-9b3e-05d11921d46a"
      }
    },
    "owner": "asura08"
  }
}

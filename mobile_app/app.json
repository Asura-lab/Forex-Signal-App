    "android": {
      "package": "com.asuralab.predictrix",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1a237e"
      }
    },
    "extra": {
      "eas": {
        "projectId": "b1fff19c-7b43-49f0-89dc-a3d6f2ed5c33"
